  "repositories": [
  ],
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "mockery/mockery": "@stable"
  },
  "autoload": {
    "psr-4": {
      "Exads\\": "src/Exads",
      "Exads\\AdLib\\": "vendor/exads/phpincludes/src"
